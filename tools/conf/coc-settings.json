{
	"suggest.floatConfig": {
		"border": true,
		"rounded": true,
		"focusable": false
	},

	"signature.floatConfig": {
		"border": true,
		"rounded": true,
		"focusable": false
	},

	"hover.floatConfig": {
		"border": true,
		"rounded": false,
		"focusable": false
	},

	"suggest.autoTrigger": "always",
	"suggest.noselect": true,
	"suggest.enablePreview": true,
	"suggest.timeout": 5000,

	"signature.target": "echo",

	"diagnostic.enable": false,
	"inlayHint.enable": false,

	"coc.source.coc-snippets.enable": false,

    "list.autoResize": false,
    "list.maxHeight": 15,
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.source.grep.defaultOptions": ["--interactive", "--no-sort", "--regex"],

	"languageserver": {
		"clangd": {
			"command": "clangd",
			"rootPatterns": [".git", ".svn", ".hg", ".root", ".project"],
			"filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
			"initializationOptions": {
			}
		}
	},

