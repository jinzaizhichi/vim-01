{
	"suggest.floatConfig": {
		"border": true,
		"rounded": true,
		"focusable": false
	},

	"signature.floatConfig": {
		"border": true,
		"rounded": true,
		"focusable": false
	},

	"hover.floatConfig": {
		"border": true,
		"rounded": false,
		"focusable": false
	},

	"suggest.autoTrigger": "always",
	"suggest.noselect": true,
	"suggest.enablePreview": true,
	"suggest.timeout": 5000,

	"signature.target": "echo",

	"diagnostic.enable": false,
	"inlayHint.enable": false,

	"coc.source.coc-snippets.enable": false,

    "list.autoResize": false,
    "list.maxHeight": 15,
    "list.source.grep.defaultArgs": ["-smartcase", "-regex"],
    "list.source.grep.defaultOptions": ["--interactive", "--no-sort", "--regex"],

	"languageserver": {
		"clangd": {
			"command": "clangd",
			"rootPatterns": [".git", ".svn", ".hg", ".root", ".project"],
			"filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
			"initializationOptions": {
			}
		}
	},

    "suggest.completionItemKindLabels": {
        "class": "\uf0e8",
        "color": "\ue22b",
        "constant": "\uf8ff",
        "default": "\uf29c",
        "event": "\ufacd",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "field": "\uf93d",
        "file": "\uf471",
        "folder": "\uf115",
        "function": "\uf794",
        "interface": "\ufa52",
        "keyword": "\uf893",
        "method": "\uf6a6",
        "module": "\uf668",
        "operator": "\uf915",
        "property": "\ufab6",
        "reference": "\uf87a",
        "snippet": "\uf64d",
        "struct": "\ufb44",
        "text": "\ue612",
        "typeParameter": "\uf278",
        "unit": "\uf475",
        "value": "\uf8a3",
        "variable": "\uf71b"
    }
}



